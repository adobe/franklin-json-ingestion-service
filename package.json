{
  "name": "@adobe/franklin-json-ingestion-service",
  "version": "2.0.4",
  "private": true,
  "description": "Franklin JSON Ingestion Service",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon",
    "integration": "node ./test/dev/server.mjs",
    "test": "c8 mocha  -i -g 'Post-Deploy'",
    "test-postdeploy": "mocha -g 'Post-Deploy'",
    "lint": "eslint .",
    "lintfix": "eslint --fix .",
    "semantic-release": "semantic-release",
    "build": "hedy -v",
    "localstackfix": "node ./test/aws/localstack-fix.js",
    "deploy-localstack": "hedy -v; node ./test/aws/localstack-fix.js",
    "deploy": "hedy -v --deploy --test",
    "deploy-routes": "hedy --no-build --no-hints -l major",
    "deploy-ci": "hedy -v --deploy --test --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci --cleanup-ci 24h --timeout 300000 --memory 2048",
    "prepare": "husky install"
  },
  "wsk": {
    "nodeVersion": 18,
    "target": "aws",
    "name": "helix-services/json-ingestion-service@${version}",
    "params-file": [
      "invalidate.env"
    ],
    "testUrl": "/_status_check/healthcheck.json",
    "memory": 256,
    "awsRole!important": "arn:aws:iam::480147251892:role/franklin-role-lambda-headless",
    "_awsDeployTemplate": "helix-tier3-deploy-template",
    "fastlyServiceId!important": ""
  },
  "mocha": {
    "spec": "test/**/*.test.js",
    "require": "test/setup-env.js",
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/franklin-json-ingestion-service"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/franklin-json-ingestion-service/issues"
  },
  "homepage": "https://github.com/adobe/franklin-json-ingestion-service#readme",
  "dependencies": {
    "@adobe/fetch": "4.1.0",
    "@adobe/helix-shared-process-queue": "3.0.0",
    "@adobe/helix-shared-wrap": "2.0.0",
    "@adobe/helix-status": "10.0.10",
    "@adobe/helix-universal-logger": "3.0.11",
    "@aws-sdk/client-s3": "3.427.0",
    "@aws-sdk/client-sqs": "3.427.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "2.0.3",
    "@adobe/helix-deploy": "9.3.6",
    "@adobe/helix-universal": "4.3.1",
    "@adobe/semantic-release-coralogix": "1.1.26",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@stryker-mutator/core": "^7.0.0",
    "@stryker-mutator/mocha-runner": "^7.0.0",
    "adm-zip": "^0.5.10",
    "aws-sdk-client-mock": "^3.0.0",
    "c8": "8.0.1",
    "dotenv": "16.3.1",
    "eslint": "8.51.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.28.1",
    "husky": "8.0.3",
    "junit-report-builder": "3.0.1",
    "lint-staged": "14.0.1",
    "mocha": "10.2.0",
    "mocha-multi-reporters": "1.5.1",
    "nock": "^13.3.0",
    "nodemon": "3.0.1",
    "semantic-release": "22.0.5"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "nodemonConfig": {
    "exec": "node --inspect ./test/dev/server.mjs",
    "watch": [
      ".env",
      "."
    ],
    "ext": ".js, .cjs, .ejs, .css"
  }
}
